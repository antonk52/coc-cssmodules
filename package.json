{
    "name": "coc-cssmodules",
    "version": "1.0.0",
    "description": "css modules autocompletion and go to definition coc.nvim plugin",
    "main": "lib/index.js",
    "scripts": {
        "clean": "rimraf lib",
        "watch": "webpack --watch",
        "watch:types": "tsc --noEmit --watch",
        "types": "tsc --noEmit",
        "build": "webpack",
        "prepare": "npm-run-all clean build",
        "test": "jest"
    },
    "keywords": [
        "coc.nvim",
        "css-modules",
        "sass",
        "scss",
        "less"
    ],
    "files": [
        "lib/*"
    ],
    "engines": {
        "coc": ">= 0.0.70"
    },
    "configuration": {
        "type": "object",
        "title": "cssmodules",
        "properties": {
            "cssmodules.camelCase": {
                "default": false,
                "description": "Enable tsserver extension",
                "oneOf": [
                    {
                        "type": "boolean"
                    },
                    {
                        "type": "string",
                        "enum": ["dashes"]
                    }
                ]
            }
        }
    },
    "author": "antonk52",
    "repository": {
        "type": "git",
        "url": "https://github.com/antonk52/coc-cssmodules.git"
    },
    "license": "MIT",
    "devDependencies": {
        "@chemzqm/tsconfig": "0.0.3",
        "@types/lodash.camelcase": "^4.3.6",
        "@types/node": "^13.9.0",
        "coc.nvim": "0.0.75",
        "jest": "^26.6.3",
        "lodash.camelcase": "^4.3.0",
        "npm-run-all": "^4.1.5",
        "rimraf": "^3.0.2",
        "ts-jest": "^26.4.4",
        "ts-loader": "^6.2.1",
        "typescript": "^3.8.3",
        "vscode-uri": "^2.1.1",
        "webpack": "^4.42.0",
        "webpack-cli": "^3.3.11"
    },
    "dependencies": {
        "postcss": "^8.1.6",
        "postcss-less": "^4.0.0",
        "postcss-sass": "^0.4.4",
        "postcss-scss": "^3.0.4"
    }
}
